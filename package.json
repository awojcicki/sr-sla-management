{
  "name": "sr-sla-management-app",
  "version": "1.0.0",
  "description": "Smartrecruiters sla management app",
  "engines": {
    "node": "8.10.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run build:client && node index.js",
    "build:client": "cd client-app && npm install && ./node_modules/.bin/webpack --env=dev",
    "webpack:watch": "cd client-app && ./node_modules/.bin/webpack -w --env=dev",
    "test": "mocha",
    "postinstall": "npm run build:client"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "bootstrap": "4.0.0",
    "express": "4.15.2",
    "pg": "^7.4.1",
    "pug": "2.0.1"
  },
  "devDependencies": {
    "mocha": "^5.0.4",
    "request": "2.81.0",
    "tape": "4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartrecruiters/sr-sla-management"
  },
  "author": "42@smartrecruiters.com",
  "license": "ISC"
}
